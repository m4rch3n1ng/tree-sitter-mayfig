{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mayfig",
      "camelcase": "Mayfig",
      "title": "mayfig",
      "scope": "source.mayfig",
      "file-types": [
        "mf"
      ],
      "injection-regex": "^mayfig$",
      "class-name": "TreeSitterMayfig"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "a tree-sitter grammar for mayfig",
    "authors": [
      {
        "name": "may"
      }
    ],
    "links": {
      "repository": "https://github.com/m4rch3n1ng/tree-sitter-mayfig"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}