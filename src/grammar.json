{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mayfig",
  "rules": {
    "config": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "#"
            },
            {
              "type": "REPEAT",
              "content": {
                "type": "PATTERN",
                "value": "."
              }
            }
          ]
        }
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "STRING",
          "value": "hello"
        },
        {
          "type": "SYMBOL",
          "name": "kv"
        }
      ]
    },
    "escaped": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "PATTERN",
            "value": "."
          }
        ]
      }
    },
    "_string_content": {
      "type": "IMMEDIATE_TOKEN",
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "PATTERN",
          "value": "[^\\x00-\\x1F\\x22\\x5c\\x7F]"
        }
      }
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_string_content"
              },
              {
                "type": "SYMBOL",
                "name": "escaped"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "bool": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "int": {
      "type": "PATTERN",
      "value": "[+-]?[0-9]+"
    },
    "float": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": ".inf"
        },
        {
          "type": "STRING",
          "value": "+.inf"
        },
        {
          "type": "STRING",
          "value": "-.inf"
        },
        {
          "type": "PATTERN",
          "value": "[+-]?[0-9]+[eE][+-]?[0-9]+"
        },
        {
          "type": "PATTERN",
          "value": "[+-]?([0-9]+\\.[0-9]*|\\.[0-9]+)([eE][+-]?[0-9]+)?"
        }
      ]
    },
    "_seq_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_value"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        },
        {
          "type": "STRING",
          "value": ","
        }
      ]
    },
    "seq": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_seq_value"
          }
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "tagged": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "string"
          },
          {
            "type": "SYMBOL",
            "name": "seq"
          }
        ]
      }
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "tagged"
        },
        {
          "type": "SYMBOL",
          "name": "seq"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "bool"
        },
        {
          "type": "SYMBOL",
          "name": "int"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        }
      ]
    },
    "bare_key": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_+-]*"
    },
    "tagged_key": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "bare_key"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "string"
              },
              "named": true,
              "value": "quoted_key"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "seq"
        }
      ]
    },
    "_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "string"
          },
          "named": true,
          "value": "quoted_key"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "tagged_key"
          },
          "named": true,
          "value": "tagged"
        },
        {
          "type": "SYMBOL",
          "name": "bare_key"
        },
        {
          "type": "SYMBOL",
          "name": "int"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
          "name": "seq"
        }
      ]
    },
    "kv": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_key"
            },
            {
              "type": "STRING",
              "value": "="
            },
            {
              "type": "SYMBOL",
              "name": "_value"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_key"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "STRING",
                  "value": "="
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "map"
            }
          ]
        }
      ]
    },
    "map": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\r\\t]"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}